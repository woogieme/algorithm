package day0111;

public class Test12 {
	public static void main(String[] args) {
		int age=23;
		String answer = "";
        int tmp=0;
        while (age>0){
            tmp=age%10;
            age/=10;
            char test =(char)(tmp +'0');
            System.out.println(test);
            answer +=Character.toString(test);
        }
        System.out.println(answer);
	}
}
