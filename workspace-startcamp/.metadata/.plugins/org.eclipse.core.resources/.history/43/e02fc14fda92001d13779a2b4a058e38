package day0111;

public class Test12 {
	public static void main(String[] args) {
		int n=10;
		int answer=0;
        while(true){
            if (n%6==0){
                answer=n/6;
                
                break;
            }
            else{
            	System.out.println(n);
                n+=6;
            }
        }
        System.out.println(answer);
	}
}
